@model App.Web.Areas.Admin.ViewModels.Contact.ListItemContactVM

<div class="modal fade" id="exampleModal-@Model.RowIndex" tabindex="-1" aria-labelledby="exampleModalLabel" aria-hidden="true">
    <div class="modal-dialog">
        <div class="modal-content">
            <div class="modal-header">
                <h5 class="modal-title" id="exampleModalLabel">Modal title</h5>
                <button type="button" class="close" data-dismiss="modal" aria-label="Close">
                    <span aria-hidden="true">&times;</span>
                </button>
            </div>
            <div class="modal-dialog-scrollable modal-dialog">
                @Model.Content
            </div>
        </div>
    </div>
</div>